        "sabre",
        "skeleton"
    ],
    "homepage": "http://sabre.io/dav/",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.2"
    },
    "authors": [
        {
            "name": "Evert Pot",
            "email": "me@evertpot.com",
            "homepage": "https://evertpot.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "forum": "https://groups.google.com/group/sabredav-discuss",
        "source": "https://github.com/fruux/sabre-skel"
    },
    "autoload": {
